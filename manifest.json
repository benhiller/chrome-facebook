{
  "name": "Facebook for Google Chrome",
  "description": "Extension which lets you read your Facebook news feed and wall. You can also post status updates.",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "version": "1.4.3",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_title": "Facebook",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs", "http://*.facebook.com/"
  ]
}
